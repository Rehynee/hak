local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("NYX MAXIMA", "DarkTheme")
--
local Character = Window:NewTab("Character")
local Hubs = Window:NewTab("Hubs")
local Visual = Window:NewTab("Visual")
local Credits = Window:NewTab("Credits")
--
local Body = Character:NewSection("Body")
local Sect = Hubs:NewSection("--")
local Sectit = Visual:NewSection("---")
local Maker = Credits:NewSection("gugugaga")

Body:NewSlider("Speed", "0 - 500", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Body:NewSlider("JumpPower", "0 - 250", 250, 0, function(s) -- 250 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
Body:NewButton("ClickTP", "Click to TP", function()
        mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Click Teleport"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)
Sect:NewButton("Infinite Yield", "Admin Commands", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)
Sect:NewButton("MurderMystery", "MurderMystery", function()
   loadstring(game:HttpGet(("https://raw.githubusercontent.com/MarsQQ/ScriptHubScripts/master/MM2%20Admin%20Panel")),true)()
end)
Sect:NewButton("DOORS", "DOORS", function()
   loadstring(game:HttpGet(("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Doors/Script.lua")),true)()
end)
Sectit:NewButton("Invisibility", "Turn invisible", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostPlayer352/Test4/main/Invisible%20Gui"))()
end)
Sectit:NewButton("ESP", "SEE EVERYONE", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua',true))()
end)
Sectit:NewSlider("Field Of View", "0 - 1000", 1000, 0, function(x) -- 1000 (MaxValue) | 0 (MinValue)
    game.Workspace.CurrentCamera.FieldOfView = x
end)
Maker:NewButton("Made By", "Click to see dc tag", function()
local New = Credits:NewSection("Rehyne#4419")
local Net = Credits:NewSection("Periodtttt")
end)
